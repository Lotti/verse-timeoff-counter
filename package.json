{
  "name": "verse-timeoff-counter",
  "version": "0.0.5",
  "description": "Add TimeOff counter capabilities to Verse Calendar ",
  "main": "public/js/main.js",
  "homepage": "./",
  "scripts": {
    "react": "react-scripts start",
    "react:build": "react-scripts build",
    "build": "npm run react:build && web-ext build -s=./build --overwrite-dest",
    "lint": "web-ext lint -s=./build",
    "sign": "web-ext sign -s=./build",
    "start": "web-ext run -s=./build",
    "dev": "web-ext run -s=./build -p=dev --keep-profile-changes"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/valerio-riva/verse-timeoff-counter.git"
  },
  "author": "Valerio Riva",
  "license": "MIT",
  "devDependencies": {
    "web-ext": "^4.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^3.26.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
